local redzlib = loadstring(game:HttpGet("https://raw.githubusercontent.com/tbao143/Library-ui/refs/heads/main/Redzhubui"))()

--// Manox Hub Full Loading Screen by GPT-5

local TweenService = game:GetService("TweenService")
local Lighting = game:GetService("Lighting")

-- GUI principal
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "ChaosFullLoading"
ScreenGui.IgnoreGuiInset = true
ScreenGui.ResetOnSpawn = false
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
ScreenGui.Parent = game:GetService("CoreGui")

-- Fundo com imagem cobrindo tudo
local Background = Instance.new("ImageLabel")
BaTABckground.Size = UDim2.new(1, 0, 1, 0)
Background.Position = UDim2.new(0, 0, 0, 0)
Background.BackgroundTransparency = 1
Background.Image = "rbxassetid://139940007638134" -- üñºÔ∏è ID da imagem de fundo
Background.ImageTransparency = 0
Background.ScaleType = Enum.ScaleType.Crop
Background.Parent = ScreenGui

-- Efeito de blur suave
local Blur = Instance.new("BlurEffect")
Blur.Size = 12
Blur.Parent = Lighting

-- üéµ Som ambiente
local Sound = Instance.new("Sound")
Sound.SoundId = "rbxassetid://1841398064" -- coloque aqui o ID do som de fundo
Sound.Volume = 1
Sound.Looped = false
Sound.Parent = workspace
Sound:Play()

-- Fundo da barra
local BarBackground = Instance.new("Frame")
BarBackground.AnchorPoint = Vector2.new(0.5, 0.5)
BarBackground.Position = UDim2.new(0.5, 0, 0.82, 0)
BarBackground.Size = UDim2.new(0.5, 0, 0.04, 0)
BarBackground.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
BarBackground.BorderSizePixel = 0
BarBackground.BackgroundTransparency = 0.25
BarBackground.Parent = ScreenGui

-- Barra de progresso
local ProgressBar = Instance.new("Frame")
ProgressBar.Size = UDim2.new(0, 0, 1, 0)
ProgressBar.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
ProgressBar.BorderSizePixel = 0
ProgressBar.Parent = BarBackground

-- Texto de porcentagem
local PercentText = Instance.new("TextLabel")
PercentText.AnchorPoint = Vector2.new(0.5, 0.5)
PercentText.Position = UDim2.new(0.5, 0, 0.73, 0)
PercentText.Size = UDim2.new(0, 400, 0, 60)
PercentText.Text = "Loading... 0%"
PercentText.TextColor3 = Color3.fromRGB(255, 255, 255)
PercentText.Font = Enum.Font.GothamBold
PercentText.TextSize = 36
PercentText.BackgroundTransparency = 1
PercentText.Parent = ScreenGui

-- üïí 8 segundos = 100 etapas -> 0.08s por porcentagem
for i = 1, 100 do
	local goal = { Size = UDim2.new(i / 100, 0, 1, 0) }
	TweenService:Create(ProgressBar, TweenInfo.new(0.08), goal):Play()
	PercentText.Text = "Loading... " .. i .. "%"
	task.wait(0.08)
end

-- Fade-out final (1s)
TweenService:Create(Background, TweenInfo.new(1), { ImageTransparency = 1 }):Play()
TweenService:Create(BarBackground, TweenInfo.new(1), { BackgroundTransparency = 1 }):Play()
TweenService:Create(ProgressBar, TweenInfo.new(1), { BackgroundTransparency = 1 }):Play()
TweenService:Create(PercentText, TweenInfo.new(1), { TextTransparency = 1 }):Play()

task.wait(1)
Blur:Destroy()
ScreenGui:Destroy()

if Sound then
	TweenService:Create(Sound, TweenInfo.new(1), { Volume = 0 }):Play()
	task.wait(1)
	Sound:Destroy()
end

