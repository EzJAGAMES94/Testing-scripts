Tab:AddTextBox({
    Name = "Insira o ID do √Åudio ou M√∫sica",
    Description = "Digite o ID do √°udio",
    PlaceholderText = "ID do √°udio",
    ClearTextOnFocus = true,

    Callback = function(value)
        local num = tonumber(value)
        if num then
            selectedAudioID = num
        else
            warn("ID inv√°lido!")
        end
    end
})

---------------------------------------------------------------
-- Dropdown (lista de √°udios) ‚Äî voc√™ preenche depois
