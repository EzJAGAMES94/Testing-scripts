---------------------------------------------------------------
-- Se√ß√£o de Loop
---------------------------------------------------------------
Tab:AddSection("Loop de √Åudio")

-- Fun√ß√£o que toca o √°udio em loop
local function playLoopedAudio()
    while audioLoop do
        if selectedAudioID then

            -- SERVER SIDE
            local gunEvent = game:GetService("ReplicatedStorage"):FindFirstChild("1Gu1nSound1s", true)
            if gunEvent then
                gunEvent:FireServer(workspace, selectedAudioID, 1)
            end

            -- CLIENT SIDE
            local sound = Instance.new("Sound")
            sound.SoundId = "rbxassetid://" .. selectedAudioID
            sound.Parent = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
            sound:Play()

        else
            warn("Nenhum √°udio selecionado!")
        end

        task.wait(0.5) -- delay para evitar overload
    end
end

Tab:AddToggle({
    Name = "Loop Tocar √Åudio",
    Description = "Ativa o loop do √°udio",
    Default = false,
    Flag = "audio_loop",

    Callback = function(value)
        audioLoop = value
        if audioLoop then
            task.spawn(playLoopedAudio)
        end
    end
})

Tab:AddParagraph("Info", "Loop de tocar √°udio (Todos players do server ouvem)")

---------------------------------------------------------------
-- Fun√ß√£o para tocar o √°udio apenas uma vez
---------------------------------------------------------------
local function playAudio()
    if selectedAudioID then
        -- SERVER SIDE
        local gunEvent = game:GetService("ReplicatedStorage"):FindFirstChild("1Gu1nSound1s", true)
        if gunEvent then
            gunEvent:FireServer(workspace, selectedAudioID, 1)
        end

        -- CLIENT SIDE
        local sound = Instance.new("Sound")
        sound.SoundId = "rbxassetid://" .. selectedAudioID
        sound.Parent = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
        sound:Play()
    else
        warn("Nenhum √°udio selecionado!")
    end
end

---------------------------------------------------------------
-- Bot√£o para tocar o √°udio
---------------------------------------------------------------
Tab:AddSection("Tocar √Åudio")

Tab:AddButton({
    Name = "Tocar √Åudio",
    Callback = function()
        playAudio()
    end
})

---------------------------------------------------------------
-- Fun√ß√µes Extras (Estourar ouvido geral)
---------------------------------------------------------------
local audioID = 6314880174 -- ID fixo
local ReplicatedStorage = game:GetService("ReplicatedStorage")

-- Client + Server Sound
local function Audio_All_ClientSide(ID)
    local function CheckFolderAudioAll()
        local FolderAudio = workspace:FindFirstChild("Audio all client")
        if not FolderAudio then
            FolderAudio = Instance.new("Folder")
            FolderAudio.Name = "Audio all client"
            FolderAudio.Parent = workspace
        end
        return FolderAudio
    end

    local function CreateSound(ID)
        if type(ID) ~= "number" then
            warn("Insira um n√∫mero v√°lido!")
            return
        end

        local Folder = CheckFolderAudioAll()

        local Sound = Instance.new("Sound")
        Sound.SoundId = "rbxassetid://" .. ID
        Sound.Volume = 1
        Sound.Looped = false
        Sound.Parent = Folder

        Sound:Play()
        task.wait(1)
        Sound:Destroy()
    end

    CreateSound(ID)
end

local function Audio_All_ServerSide(ID)
    if type(ID) ~= "number" then
        warn("Insira um n√∫mero v√°lido!")
        return
    end

    local GunSoundEvent = ReplicatedStorage:FindFirstChild("1Gu1nSound1s", true)
    if GunSoundEvent then
        GunSoundEvent:FireServer(workspace, ID, 1)
    end
end

---------------------------------------------------------------
-- Toggle Spam
---------------------------------------------------------------
Tab:AddToggle({
    Name = "Estourar ouvido de geral KK",
    Description = "Toca √°udio repetidamente para todos",
    Default = false,
    Flag = "audio_spam",

    Callback = function(value)
        getgenv().Audio_All_loop_fast = value

        while getgenv().Audio_All_loop_fast do
            Audio_All_ServerSide(audioID)

            task.spawn(function()
                Audio_All_ClientSide(audioID)
            end)

            task.wait(0.03) -- super r√°pido
        end
    end
})

Tab:AddParagraph("Info", "Todos do server ouvem o √°udio")
